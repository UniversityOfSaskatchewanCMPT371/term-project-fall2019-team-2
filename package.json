{
  "name": "timeline",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/d3": "^5.7.2",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/sinon": "^7.5.0",
    "@types/timsort": "^0.3.0",
    "bootstrap": "^4.3.1",
    "d3": "^5.12.0",
    "d3-dsv": "^1.1.1",
    "d3-time-format": "^2.1.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.2",
    "fast-sort": "^1.6.0",
    "jest": "^24.9.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "object-sizeof": "^1.5.2",
    "react": "^16.10.2",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-test-renderer": "^16.10.2",
    "sinon": "^7.5.0",
    "typescript": "^3.6.3",
    "cross-env": "6.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage --testResultsProcessor ./node_modules/jest-sonar-reporter",
    "eject": "react-scripts eject",
    "wint": "eslint src/**",
    "lint": "eslint 'src/**'",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "electron": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && cross-env ELECTRON_START_URL=http://localhost:3000 electron .\"",
    "package": "npm run build && electron-builder build --mac --linux --win -c.extraMetadata.main=build/electron.js --publish never",
    "package-macos": "npm run build && electron-builder build --mac -c.extraMetadata.main=build/electron.js --publish never",
    "package-win-linux": "npm run build && electron-builder build --win --linux -c.extraMetadata.main=build/electron.js --publish never",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "appId": "com.example.electron-cra"
  },
  "eslintConfig": {
    "extends": "google"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "electron": "^7.1.0",
    "electron-builder": "^22.1.0",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.16.0",
    "jest-sonar-reporter": "^2.0.0",
    "concurrently": "4.1.2",
    "wait-on": "3.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "lcov"
    ]
  },
  "main": "public/electron.js",
  "homepage": "./"
}
